import aktual.budget.model.PayeeId;

CREATE TABLE IF NOT EXISTS payee_mapping(
  id TEXT AS PayeeId PRIMARY KEY,
  targetId TEXT AS PayeeId
);

insert:
  INSERT OR REPLACE
    INTO payee_mapping(id, targetId)
    VALUES(?, ?);

getIdsByTargetId:
  SELECT id
    FROM payee_mapping
    WHERE targetId = ?;
